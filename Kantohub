})

--Mastered Cedrick

MasteredSection:AddButton({
	Name = "Mastered Cedrick (Speeding Limit)",
	Callback = function()
		_G.settings = {
			["RedStartupId"] = "rbxassetid://1177475221",
			["RedHitId"] = "rbxassetid://8625377966",
		}
		loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/BaldyToSorcerer/main/Latest.lua"))()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/PurpleCone/gojo-max-speed/main/gojo%20max%20speed.txt"))()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Miks122/asdsad/main/asdasd"))()
  	end,
    Image = "rbxassetid://18154629221"
})

--Mastered Mikael

MasteredSection:AddButton({
	Name = "Mastered Mikael",
	Callback = function()
		loadstring(game:HttpGet("https://pastebin.com/raw/xiUjbnbx"))()
		loadstring(game:HttpGet("https://pastebin.com/raw/V8ACJxqq"))()
		_G.settings = {
			["RedStartupId"] = "rbxassetid://1177475221",
			["RedHitId"] = "rbxassetid://8625377966",
		}
		loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/BaldyToSorcerer/main/Latest.lua"))()
		wait(.9)
        loadstring(game:HttpGet("https://pastebin.com/raw/V8ACJxqq"))()
  	end,
    Image = "rbxassetid://18182262939"
})


