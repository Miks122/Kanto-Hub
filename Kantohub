local LocalizationService = game:GetService("LocalizationService")
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer

if game.PlaceId == 10449761463 then
local Window = OrionLib:MakeWindow({Name = "Kanto Hub", IntroText = "Welcome to the Kanto.", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", Icon = "rbxassetid://79769986663362"})


OrionLib:MakeNotification({
	Name = "Welcome to the Kanto",
	Content = "Logged in as"..Player.Name..".",
	Image = "rbxassetid://79769986663362",
	Time = 5
})

--Main

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://18737355499",
	PremiumOnly = false
})

local MainSection = MainTab:AddSection({
	Name = "Main"
})

MainSection:AddButton({
	Name = "Infinite Yield",
	Callback = function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
  	end    
})

MainSection:AddSlider({
	Name = "WalkSpeed (Probably Works)",
	Min = 16,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		if true then
			game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = Value
		end
	end    
})

MainSection:AddButton({
	Name = "Magma Hub",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Commandcracker/Magma-Hub/main/main.lua'))()
  	end    
})

--Special

local SpecialTab = Window:MakeTab({
	Name = "Special",
	Icon = "rbxassetid://12558753709",
	PremiumOnly = false
})

local SpecialSection = SpecialTab:AddSection({
	Name = "Special"
})

--Mixed Characters

local MixedSection = SpecialTab:AddSection({
	Name = "Mixed Characters"
})


--Mixed Powers

MixedSection:AddButton({
	Name = "Mixed Powers (Mikael and Ace)",
	Callback = function()
		loadstring(game:HttpGet("https://pastebin.com/raw/xiUjbnbx"))()
		loadstring(game:HttpGet("https://pastebin.com/raw/V8ACJxqq"))()
		wait(.9)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Miks122/adsdasdasda/main/asdasd"))()
  	end,
    Image = "rbxassetid://18154629221"
})

MixedSection:AddButton({
	Name = "Colorful Hunger (Gojo and Chan)",
	Callback = function()
		_G.settings = {
			["RedStartupId"] = "rbxassetid://1177475221",
			["RedHitId"] = "rbxassetid://8625377966",
		}
		loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/BaldyToSorcerer/main/Latest.lua"))()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Miks122/fassfaasfafs/main/asdasdasdasd"))()

  	end,
    Image = "rbxassetid://18154629221"
})

--Double Trouble

MixedSection:AddButton({
	Name = "Double Trouble",
	Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/uziHCYyU"))()
  	end,
    Image = "rbxassetid://8187306832"
})


